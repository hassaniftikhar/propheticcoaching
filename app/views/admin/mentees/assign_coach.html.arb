table_for mentee do
  column :name
  column :email
  column :coach_name do |mentee|
  		mentee.coaches.collect {|coach| (link_to coach.name, admin_user_path(coach))}.join(", ").html_safe
  end
end


"Assign Coach"

form_for [:admin, mentee] do |f|
		f.select :coach_ids, User.coach.all.collect {|c| [c.name, c.id]}, {}, :multiple => true
  f.button "Select/Change Coach"
end