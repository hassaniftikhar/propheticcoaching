table_for mentees do
  column :name
  column :email
  column :coach_name do |m|
    link_to m.coach.name, admin_user_path(m.coach_id) if m.coach
  end
end


"Assign Coach"

form_for Mentee.new, url: assign_multiple_coaches_admin_mentees_path do |f|
    f.hidden_field 'mentees_id_list', :value=>selection
  f.select :coach_id, options_from_collection_for_select(User.coaches.all, 'id', 'name')
  f.button "Select/Change Coach"
end
