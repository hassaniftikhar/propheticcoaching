if mentees
    table_for mentees do
      column :name
      column :email
      column :coach_name do |mentee|
          mentee.coaches.order("first_name asc").collect {|coach| (link_to coach.name, admin_user_path(coach))}.join(", ").html_safe
      end
    end


    "Assign Coach"
    render "assign_multi_coachs"

end