<% unless current_user %>
  <div class="call-out margin-top-100">
    <div class="container">
      <div class="row">
        <div class="col-sm-8">
          <p class="call-out-text">Experience the Prophetic Coaching for 30-Days - Prophetic coaching system</p>
        </div>
