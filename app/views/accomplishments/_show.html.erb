<h3> Listing Accomplishments </h3>
<table class="table table-striped" >
  <thead>
  <tr>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @accomplishments.each do |accomplishment| %>
    <tr>
      <td><%= accomplishment.body %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= paginate @accomplishments %>