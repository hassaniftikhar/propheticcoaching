<h2 class="tablehead"> Google Calendars </h2>
<table class="table table-striped">
  <thead>
  <tr>
    <th>Google Calendar Address</th>
<!--     <th>Google Calendar API Key</th>
    <th>Google Calendar ID</th> -->
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% @google_events.each do |google_event| %>
    <tr>
      <td><%= google_event.url %></td>
<!--       <td><%= google_event.google_calendar_api_key %></td>
      <td><%= google_event.google_calendar_id %></td> -->
      <td>
        <% if can? :edit, @google_events %>

          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      [:edit, google_event.profile, google_event], :class => 'btn btn-mini google_event' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      [google_event.profile, google_event],
                      :method => :delete,
                      :data => {:confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?'))},
                      :class => 'btn btn-mini btn-danger' %>
        <% end %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= paginate @google_events %>

<div id="create_event_dialog" class="dialog" style="display:none;">
  <div id="create_event">
  </div>
</div>

<div id="create_google_event_dialog" class="dialog" style="display:none;">
  <div id="create_google_event">
  </div>

</div>