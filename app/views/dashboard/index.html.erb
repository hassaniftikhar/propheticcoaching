<h2>Latest Resources</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Created at</th>
      <th></th>

    </tr>
  </thead>

  <tbody>
    <% @latest_resources.each do |latest_resource| %>
      <tr>
        <td><%= link_to latest_resource.name, ebook_path(latest_resource) %></td>
        <td><%= latest_resource.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<h2>Appointments</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Start Time</th>
      <th>End Time</th>
      <th>Created at</th>
      <th></th>

    </tr>
  </thead>

  <tbody>
    <% @coach_events.each do |coach_event| %>
      <tr>
        <td><%= coach_event.title %></td>
        <td><%= coach_event.description %></td>
        <td><%= coach_event.starttime %></td>
        <td><%= coach_event.endtime %></td>
        <td><%= coach_event.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>


